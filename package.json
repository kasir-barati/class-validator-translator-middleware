{
    "name": "class-validator-translator-middleware",
    "version": "1.0.0",
    "description": "A middleware to convert class-validator messages to locale languages using similar i18n pattern",
    "main": "dist/index.js",
    "scripts": {
        "prepublish": "npm run build",
        "build": "rimraf ./dist && tsc -p tsconfig.build.json",
        "start:dev": "nodemon",
        "format": "prettier --write '**/*.js' '**/*.ts' '**/*.json'",
        "test": "jest",
        "prepare": "husky install",
        "lint": "eslint ."
    },
    "keywords": [
        "translate-errors",
        "class-validator",
        "multi language",
        "i18n"
    ],
    "author": "nodejsdeveloperskh",
    "license": "GPL-3.0",
    "devDependencies": {
        "@babel/core": "^7.16.5",
        "@babel/preset-env": "^7.16.5",
        "@babel/preset-typescript": "^7.16.5",
        "@types/jest": "^27.0.3",
        "@types/lodash": "^4.14.178",
        "@types/node": "^17.0.2",
        "@typescript-eslint/eslint-plugin": "^5.8.0",
        "@typescript-eslint/parser": "^5.8.0",
        "babel-jest": "^27.4.5",
        "eslint": "^8.5.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^7.0.4",
        "jest": "^27.4.5",
        "nodemon": "^2.0.15",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "ts-jest": "^27.1.2",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.4"
    },
    "dependencies": {
        "@types/express": "^4.17.13",
        "@vitalets/google-translate-api": "^7.0.0",
        "class-validator": "^0.13.2",
        "lodash": "^4.17.21"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nodejsdeveloperskh/class-validator-translator-middleware"
    },
    "types": "./dist/index.d.ts",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=14"
    }
}
