{
    "name": "class-validator-translator-middleware",
    "version": "0.0.7",
    "description": "A middleware to convert class-validator messages to locale languages using similar i18n pattern",
    "main": "dist/index.js",
    "scripts": {
        "prepublish": "npm run build",
        "build": "rimraf ./dist && tsc",
        "start:dev": "nodemon",
        "format": "prettier --write '**/*.js' '**/*.ts' '**/*.json'",
        "test": "jest"
    },
    "keywords": [
        "translate-errors",
        "class-validator",
        "multi language",
        "i18n"
    ],
    "author": "nodejsdeveloperskh",
    "license": "GPL-3.0",
    "devDependencies": {
        "@babel/core": "^7.16.5",
        "@babel/preset-env": "^7.16.5",
        "@babel/preset-typescript": "^7.16.5",
        "@types/jest": "^27.0.3",
        "@types/lodash": "^4.14.178",
        "@types/node": "^17.0.2",
        "babel-jest": "^27.4.5",
        "jest": "^27.4.5",
        "nodemon": "^2.0.15",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "ts-jest": "^27.1.2",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.4"
    },
    "dependencies": {
        "@types/express": "^4.17.13",
        "class-validator": "^0.13.2",
        "lodash": "^4.17.21",
        "puppeteer": "^13.0.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nodejsdeveloperskh/class-validator-translator-middleware"
    },
    "types": "./dist/index.d.ts",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=14"
    }
}
